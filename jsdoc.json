{
   "tags": {
      "allowUnknownTags": true
   },
   "plugins": ["node_modules/better-docs/typescript"],
   "source": {
      "includePattern": "\\.(jsx|js|ts|tsx)$",
      "excludePattern": "(node_modules/|docs)"
   },
   "opts": {
      "template": "node_modules/better-docs",
      "destination": "./docs/",
      "readme": "./README.md"
   },
   "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "search": true,
        "default": {
            "includeDate": true
        },
        "better-docs": {
            "name": "4Anime Scraper API",
            "title": "4Anime Scraper API",
            "hideGenerator": false,
            "navLinks": [
                {
                    "label": "Repo",
                    "href": "https://github.com/sb-gravity100/4anime-api-test"
                },
                {
                    "label": "Github",
                    "href": "https://github.com/sb-gravity100"
                }
            ]
        }
    }
}
