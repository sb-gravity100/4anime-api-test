{
  "name": "node-4anime-scraper",
  "version": "1.1.0",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sb-gravity100/4anime-api-test.git"
  },
  "author": "SB <'sbducao0@gmail.com'>",
  "license": "MIT",
  "description": "4anime API",
  "bugs": {
    "url": "https://github.com/sb-gravity100/4anime-api-test/issues"
  },
  "homepage": "https://github.com/sb-gravity100/4anime-api-test#readme",
  "scripts": {
    "transpile": "babel ./src --out-file ./dist/index.js --extensions \".js,.ts\"",
    "clean": "rimraf ./dist",
    "type": "tsc",
    "build": "npm-run-all clean transpile type",
    "watch": "babel ./src --out-file ./dist/index.js --extensions \".js,.ts\" -w",
    "publish": "np --yolo",
    "docs": "run-s docs:*",
    "docs:clean": "rimraf ./docs",
    "docs:transpile": "typedoc"
  },
  "keywords": [
    "anime",
    "downloader",
    "4anime",
    "node"
  ],
  "dependencies": {
    "aigle": "^1.14.1",
    "axios": "^0.21.1",
    "axios-retry": "^3.1.9",
    "jsdom": "^16.5.3",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/jsdom": "^16.2.10",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.37",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "typescript": "^4.2.4"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
}
