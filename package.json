{
  "name": "node-4anime-scraper",
  "version": "1.1.1",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sb-gravity100/4anime-api-test.git"
  },
  "author": "SB <'sbducao0@gmail.com'>",
  "license": "MIT",
  "description": "4anime API",
  "bugs": {
    "url": "https://github.com/sb-gravity100/4anime-api-test/issues"
  },
  "homepage": "https://github.com/sb-gravity100/4anime-api-test#readme",
  "scripts": {
    "clean": "rimraf ./dist",
    "type": "tsc",
    "watch": "tsc --watch",
    "publish": "np --yolo --no-yarn",
    "prepublish": "yarn run docs",
    "docs": "run-s docs:*",
    "docs:1clean": "rimraf ./docs",
    "docs:2transpile": "typedoc",
    "docs:3commit": "git add docs && git commit -m \"Docs updated\" ",
    "prerelease": "np prerelease --yolo --no-yarn --tag=pre"
  },
  "keywords": [
    "anime",
    "downloader",
    "4anime",
    "node"
  ],
  "dependencies": {
    "aigle": "^1.14.1",
    "axios": "^0.21.1",
    "axios-retry": "^3.1.9",
    "core-js": "^3.10.2",
    "jsdom": "^16.5.3",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/jsdom": "^16.2.10",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.41",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "typescript": "^4.2.4"
  },
  "files": [
    "dist/",
    "src/",
    "index.js",
    "test.js",
    "tsconfig.json"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
}
